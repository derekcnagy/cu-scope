<h1>Profiles</h1>

<%= render "shared/menu" %>

<div>Existing Profiles</div>

<% @profiles.each do |profile| %>
  <div id="exiting_team_name"><%= profile[:profile_name] %></div>
<% end %>

<%= form_tag('/profiles', multipart: true) do %>
  <%= text_field_tag :profile_name %>
  <%= select_tag 'team', options_from_collection_for_select(@teams, 'id', 'team_name')%>
  <%= submit_tag 'Add Profile' %>
<% end %>