<h1>Create Report</h1>


<%= render "shared/menu" %>

<div>Files Waiting to Process:</div>
<div id="files_to_process_list">
  <% @files_to_process.each do |file| %>
    <%= form_tag 'file_uploader/process_file', method: :post do %>
      <%= text_field_tag :file_name, file[:file_name], readonly: true %>
      <%= datetime_field_tag :time_ran, file[:time_ran] %>
      <%= select_tag "team", options_from_collection_for_select(@teams, 'id', 'team_name', file[:team]) %>
      <%= select_tag "profile", options_from_collection_for_select(@profiles[file[:team]], 'id', 'profile_name', file[:profile]), include_blank: 'None' %>
      <%= submit_tag "Process" %>
      <div id="test_three">Test Three</div>
    <% end %>
  <% end %>
</div>
</br>
</br>

<div id="files_to_upload">
  <%= form_tag('/file_uploader/upload', method: :put, multipart: true) do %>
    <%= file_field_tag :upload %>
    <%= submit_tag "Upload" %>
  <% end %>
</div>


<div id="test">
  <div id="test_two">Test Two</div>
  <div id="test_three">Test Three</div>
</div>